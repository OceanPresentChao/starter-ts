    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@types/node": "^17.0.45",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "typescript": "^4.9.4",
    "vite": "^2.9.15",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.15.2"
  }
